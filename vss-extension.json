{
  "manifestVersion": 1,
  "id": "team-member-randomizer",
  "version": "0.1.91",
  "name": "Team Member Randomizer",
  "description": "Team Member Randomizer for Daily Standups",
  "publisher": "enginpolat",
  "public": false,
  "categories": [
    "Azure Boards"
  ],
  "links": {
    "learn": {
      "uri": "https://github.com/microsoft/azdo-extension-team-randomizer"
    }
  },
  "icons": {
    "default": "src/icon.png"
  },
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "scopes": [
    "vso.project",
    "vso.work",
    "vso.extension.data"
  ],
  "demands": [
    "api-version/2.0"
  ],
  "content": {
    "details": {
      "path": "DETAILS.md"
    }
  },
  "branding": {
    "color": "rgb(190, 39, 3)",
    "theme": "dark"
  },
  "files": [
    {
      "path": "src/icon.png",
      "addressable": true
    },
    {
      "path": "src/main.html",
      "addressable": true
    },
    {
      "path": "src/scripts/main.js",
      "addressable": true
    },
    {
      "path": "src/scripts/popup.js",
      "addressable": true
    },
    {
      "path": "src/scripts/projectSettings.js",
      "addressable": true
    },
    {
      "path": "src/scripts/dataService.js",
      "addressable": true
    },
    {
      "path": "src/scripts/utils.js",
      "addressable": true
    },
    {
      "path": "src/styles/main.css",
      "addressable": true
    },
    {
      "path": "src/popupDialog.html",
      "addressable": true
    },
    {
      "path": "src/projectSettings.html",
      "addressable": true
    },
    {
      "path": "node_modules/azure-devops-extension-sdk",
      "addressable": true
    },
    {
      "path": "node_modules/azure-devops-extension-api",
      "addressable": true
    },
    {
      "path": "node_modules/vss-web-extension-sdk",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "popupDialog",
      "type": "ms.vss-web.control",
      "description": "The content displayed in the dialog shown in the contributed menu actions from this extension.",
      "targets": [],
      "properties": {
        "uri": "src/popupDialog.html"
      }
    },
    {
      "id": "team-member-randomizer-button",
      "type": "ms.vss-web.action",
      "description": "Team member randomizer",
      "targets": [
        "ms.vss-work-web.sprint-board-pivot-filter-menu"
      ],
      "properties": {
        "text": "",
        "title": "Team Member Randomizer",
        "icon": "src/icon.png",
        "group": "actions",
        "uri": "src/main.html",
        "registeredObjectId": "teamMemberRandomizerButton"
      }
    },
    {
      "id": "team-member-randomizer-settings",
      "type": "ms.vss-web.hub",
      "description": "Team Member Randomizer Settings",
      "targets": [
        "ms.vss-web.project-admin-hub-group"
      ],
      "properties": {
        "name": "Team Member Randomizer",
        "order": 100,
        "uri": "src/projectSettings.html"
      }
    }
  ]
}
